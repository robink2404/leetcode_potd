class Solution {
public:
    int minSwaps(vector<int>& nums) {
 int cnt=0;
 int n=nums.size();
 int one=0;
 for(int i=0;i<nums.size();i++)
 {
    if(nums[i]==1)
    one++;
 }

 int i=0;
 int j=0;
 int mini=INT_MAX;
 while(j-i<one)
 {
    if(nums[j]==0)
    cnt++;
    j++;
 }
 cout<<j<<" "<<i<<" "<<cnt;
 mini=cnt;

 while(i<n)
 {
    if(nums[j%n]==0)
    cnt++;
    if(nums[i]==0)
    cnt--;
    i++;
    j++;
    mini=min(mini,cnt);
}
 return mini;








        
    }
};
